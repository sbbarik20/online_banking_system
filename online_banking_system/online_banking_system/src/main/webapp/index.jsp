<script>
  function chk() {
    nameError = validName();
    emailError = validEmail();
    subjectError = validSubject();
    messageError = validMessage();
    if (nameError && emailError && subjectError && messageError) {
      return true;
    } else {
      return false;
    }
  }
  function validName() {
    name = $("#name").val();
    if (name == "") {
      $("#nsms").html("Name should not be blank");
      return false;
    } else {
      $("#nsms").html("");
      return true;
    }
  }

  function validEmail() {
    email = $("#email").val();
    if (email == "") {
      $("#esms").html("Email should not be blank");
      return false;
    } else {
      $("#esms").html("");
      return true;
    }
  }

  function validSubject() {
    subject = $("#subject").val();
    if (subject == "") {
      $("#ssms").html("Subject should not be blank");
      return false;
    } else {
      $("#ssms").html("");
      return true;
    }
  }

  function validMessage() {
    message = $("#message").val();
    if (message == "") {
      $("#msms").html("Message should not be blank");
      return false;
    } else {
      $("#msms").html("");
      return true;
    }
  }

</script>